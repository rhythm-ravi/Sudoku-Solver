<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.dooku.MenuController">
   <children>
      <Label text="MENU" textAlignment="CENTER" textFill="#560dff">
         <font>
            <Font name="System Italic" size="12.0" />
         </font></Label>
      <Button fx:id="primaryButton" onAction="#switchToMain" text="Launch the Solver" />
      <Button fx:id="launchSettings" onAction="#switchToSettings" text="Settings" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
